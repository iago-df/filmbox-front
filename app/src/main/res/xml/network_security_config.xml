<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- Permitir HTTP (cleartext) para el backend en desarrollo -->
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system" />
            <certificates src="user" />
        </trust-anchors>
    </base-config>
</network-security-config>
